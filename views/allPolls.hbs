
<div class="row">
{{#each polls}}
  <div class="col-3">
    <div class="card">
      <div class="card-block">
        <div class="card-title display-4"><p class="card-text">{{this.question}}</p></div>
        <a href="{{this.pollURL}}" class="btn btn-block btn-outline-success"><img src="/public/assets/vote.png" class="voteIcon" alt="Vote">View</a>
    </div>
    </div>
  </div>
{{/each}}
</div>
